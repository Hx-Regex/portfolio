[32m+[m[32mexport default function ProjectSectionClean() {[m
[32m+[m[32m  const sectionRef = useRef<HTMLElement>(null)[m
[32m+[m[32m  const projectRefs = useRef<(HTMLDivElement | null)[]>([])[m
[32m+[m
[32m+[m[32m  useEffect(() => {[m
[32m+[m[32m    // Set up refs array with proper length[m
[32m+[m[32m    projectRefs.current = projectRefs.current.slice(0, projects.length)[m
[32m+[m[41m    [m
[32m+[m[32m    const ctx = gsap.context(() => {[m
[32m+[m[32m      // Simple fade in animation for the section[m
[32m+[m[32m      gsap.from(".section-title", {[m
[32m+[m[32m        opacity: 0,[m
[32m+[m[32m        y: 20,[m
[32m+[m[32m        duration: 0.8,[m
[32m+[m[32m        ease: "power2.out",[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      // Staggered appearance for project items[m
[32m+[m[32m      gsap.from(projectRefs.current, {[m
[32m+[m[32m        opacity: 0,[m
[32m+[m[32m        y: 30,[m
[32m+[m[32m        stagger: 0.2,[m
[32m+[m[32m        duration: 0.8,[m
[32m+[m[32m        ease: "power2.out",[m
[32m+[m[32m        delay: 0.3,[m
[32m+[m[32m      })[m
[32m+[m[32m    }, sectionRef)[m
[32m+[m
[32m+[m[32m    return () => ctx.revert()[m
[32m+[m[32m  }, [])[m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <section ref={sectionRef} className="py-20" style={{ backgroundColor: "oklch(0.93 0.0058 264.53 / 90%)" }}>[m
[32m+[m[32m      <div className="container px-4 mx-auto ">[m
[32m+[m[32m        <h2 className="section-title text-3xl font-bold mb-2 text-gray-900">Featured Projects</h2>[m
