[32m+[m[32m        </p>[m
[32m+[m
[32m+[m[32m        <div className="space-y-16">[m
[32m+[m[32m          {projects.map((project, index) => ([m
[32m+[m[32m            <div[m
[32m+[m[32m              key={project.id}[m
[32m+[m[32m              ref={el => {[m
[32m+[m[32m                projectRefs.current[index] = el;[m
[32m+[m[32m              }}[m
[32m+[m[32m              className={`flex flex-col ${index % 2 === 0 ? "md:flex-row" : "md:flex-row-reverse"} gap-8 items-center`}[m
[32m+[m[32m            >[m
